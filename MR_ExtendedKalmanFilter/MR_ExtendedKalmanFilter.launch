<launch>

<node pkg="MR_ExtendedKalmanFilter" type="MR_ExtendedKalmanFilter" name="MR_ExtendedKalmanFilter">
  <param name="output_frame" value="odom_combined"/>
  <param name="base_footprint_frame" value="base_footprint"/>
  <param name="freq" value="30.0"/>
  <param name="sensor_timeout" value="1.0"/>  
  <param name="odom_used" value="true"/>
  <param name="imu_used" value="true"/>
  <param name="vo_used" value="true"/>
  <param name="gps_used" value="false"/>

  <remap from="odom" to="robot/odom" />
  <remap from="imu_data" to="robot/imu_data" />
  <remap from="vo" to="robot/vo" />
  <remap from="gps" to="robot/gps" />
</node>

</launch>
